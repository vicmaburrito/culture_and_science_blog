  <%= form_with model: @article do |form| %>
<div>
  <div>
    <%= form.label :title%><br>
    <%= form.text_field :title, placeholder: "Your title of your article here"%>
  </div>
    <div>
    <%= form.label :text%><br>
    <%= form.text_field :text, placeholder: "Write something..."%>
  </div>
    <div>
    <%= form.label :image%><br>
    <%= form.text_field :image, placeholder: "URL"%>
  </div>
  <div>
    <%= form.submit %>
  </div>
  <% end %>
  <%= link_to "back", root_path %>

</div>